{
  "ConnectionStrings": {
    //"DefaultConnection": "Server=.\\SERVER\\MSSQLSERVER01;Database=AspnetB8;User Id=aspnetb8;Password=123456;Trust Server Certificate=True;"
    "DefaultConnection": "Server=.\\SERVER\\MSSQLSERVER01;Database=AspnetB8;Trust Server Certificate=True;"

  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/web-log-.log",
          "rollingInterval": "Day"
        }
      }
    ],
  },

  "AllowedHosts": "*"
}
